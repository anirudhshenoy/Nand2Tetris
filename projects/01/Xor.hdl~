/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(a = a, out = notA);
    Not(a = b, out = notB);
    And(a = a, b = notB, out = andA);
    And(a = b, b = notA, out = andB);
    Or(a = andA, b = andB, out = out); 
}
